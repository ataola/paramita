{
  "name": "paramita",
  "version": "1.0.0",
  "description": "a css library base on sass",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "cross-env webpack --mode=production",
    "watch": "cross-env webpack --watch --mode=development",
    "sass:compact": "sass --watch src:dist/compact --style=compact",
    "sass:compressed": "sass --watch src:dist/compressed --style=compressed",
    "sass:expanded": "sass --watch src:dist/expanded --style=expanded --debug-info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ataola/paramita.git"
  },
  "keywords": [
    "sass",
    "css"
  ],
  "author": "ataola",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ataola/paramita/issues"
  },
  "homepage": "https://github.com/ataola/paramita#readme",
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.15",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.0",
    "cssnano": "^4.1.10",
    "fibers": "^5.0.0",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.4.0",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.9",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^6.7.0",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0"
  }
}
